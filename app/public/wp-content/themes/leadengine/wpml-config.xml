    </admin-texts>
	<custom-types>
        <custom-type translate="1">portfolio</custom-type>
    </custom-types>
	<taxonomies>
        <taxonomy translate="1">portfolio-category</taxonomy>
    </taxonomies>
	<custom-fields>
		<custom-field action="copy-once">_themetek_page_transparent_navbar</custom-field>
		<custom-field action="copy-once">_themetek_page_showhide_title_section</custom-field>
		<custom-field action="copy-once">_themetek_page_showhide_breadcrumbs</custom-field>
		<custom-field action="copy-once">_themetek_page_particles</custom-field>
		<custom-field action="translate">_themetek_page_subtitle</custom-field>
		<custom-field action="copy-once">page_title_align</custom-field>
		<custom-field action="copy-once">_themetek_page_title_color</custom-field>
		<custom-field action="copy-once">_themetek_page_titlebar_background</custom-field>
		<custom-field action="copy-once">_themetek_page_bgcolor</custom-field>
		<custom-field action="copy-once">_themetek_page_top_padding</custom-field>
		<custom-field action="copy-once">_themetek_page_bottom_padding</custom-field>
	</custom-fields>
	<shortcodes>
		<shortcode>
            <tag>tek_alertbox</tag>
            <attributes>
                <attribute>ab_message</attribute>
            </attributes>
        </shortcode>
		<shortcode>
            <tag>tek_appgallery</tag>
